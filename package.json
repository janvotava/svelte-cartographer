{
  "name": "svelte-cartographer",
  "version": "0.0.7",
  "homepage": "https://github.com/janvotava/svelte-cartographer",
  "scripts": {
    "dev": "vitebook dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
